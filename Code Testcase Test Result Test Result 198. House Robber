func rob(nums []int) int {
    dp := make([]int, len(nums))
    dp[0] = nums[0]
    for i := 0; i < len(nums); i++ { 
        dp[i] = nums[i]
        for j := 0; j < i - 1; j++ {
            if nums[i] + dp[j] > dp[i] {
                dp[i] = nums[i] + dp[j]
            }
        }
    }
    max := dp[0]
    for i := range dp {
        if dp[i] > max {
            max = dp[i]
        }
    }
    return max
}
